# Copy to comfyui.env and fill in the secrets before running docker compose

COMFYUI_PORT=9090
COMFYUI_HOST=0.0.0.0

# GPU binding (comma-separated indexes ex: 0 or 0,1)
CUDA_VISIBLE_DEVICES=0

# WordPress SSO shared secret (should match WP plugin option)
WP_SSO_SHARED_SECRET=replace-me

# Base URL that nginx exposes for iframe embedding
PUBLIC_BASE_URL=https://framepack.ai.cryptolabs.co.za

# FramePack assets / license
FRAMEPACK_LICENSE_KEY=
FRAMEPACK_BUNDLE_URL=https://example.com/framepack/bundles/movie-maker.zip

# Storage roots (these derive volume mounts in docker-compose)
COMFYUI_DATA_DIR=/data
COMFYUI_OUTPUT_DIR=/outputs

# HuggingFace token for model downloads (optional if cached)
HUGGING_FACE_HUB_TOKEN=

# Optional remote logging
SENTRY_DSN=

# Wrapper settings
WRAPPER_PORT=9443
WRAPPER_LOG_LEVEL=info

